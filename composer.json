{
    "name": "php-etl/spreadsheet-flow",
    "description": "This library implements the Extract-Transform-Load pattern asynchronously in PHP with the help of iterators and generators",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kiboko SAS",
            "homepage": "http://kiboko.fr"
        },
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin"
    },
    "require": {
        "php": "^8.0",
        "ext-zip": "*",
        "ext-dom": "*",
        "ext-xmlreader": "*",
        "ext-simplexml": "*",
        "php-etl/bucket": "^0.2.0",
        "psr/log": "^1.1",
        "box/spout": "^3.1",
        "php-etl/pipeline-contracts": "^0.3.0"
    },
    "autoload": {
        "psr-4": {
            "Kiboko\\Component\\Flow\\Spreadsheet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "functional\\Kiboko\\Component\\Flow\\Spreadsheet\\": "tests/functional/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "require-dev": {
        "ext-xdebug": "*",
        "adlawson/vfs": "dev-develop",
        "phpunit/phpunit": "^9.0",
        "php-etl/pipeline": "^0.4.0",
        "php-etl/phpunit-extension": "*"
    }
}

{
    "name": "php-etl/spreadsheet-flow",
    "description": "This library implements the Extract-Transform-Load pattern asynchronously in PHP with the help of iterators and generators",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kiboko SAS",
            "homepage": "http://kiboko.fr"
        },
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "infection/extension-installer": true,
            "php-http/discovery": true
        }
    },
    "require": {
        "php": "^8.2",
        "ext-zip": "*",
        "ext-dom": "*",
        "ext-xmlreader": "*",
        "ext-simplexml": "*",
        "php-etl/bucket": "*",
        "box/spout": "^3.1",
        "php-etl/pipeline-contracts": "0.4.0",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.0",
        "infection/infection": "^0.26.18",
        "rector/rector": "^0.15.23",
        "php-etl/phpunit-extension": "*"
    },
    "autoload": {
        "psr-4": {
            "Kiboko\\Component\\Flow\\Spreadsheet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "functional\\Kiboko\\Component\\Flow\\Spreadsheet\\": "tests/functional/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.2.x-dev"
        }
    }
}
